{
  "name": "komeet",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "format": "prettier --write src/",
    "cordova:prepare": "npm run build:prod && cp -r dist/* cordova-app/www/",
    "cordova:build:android": "npm run cordova:prepare && cd cordova-app && cordova build android",
    "cordova:build:ios": "npm run cordova:prepare && cd cordova-app && cordova build ios",
    "cordova:run:android": "npm run cordova:prepare && cd cordova-app && cordova run android",
    "cordova:run:ios": "npm run cordova:prepare && cd cordova-app && cordova run ios"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "flag-icons": "^7.5.0",
    "pinia": "^2.3.0",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "swiper": "^11.1.15",
    "vue": "^3.5.13",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.2.5",
    "vue3-datepicker": "^0.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/sockjs-client": "^1.5.4",
    "@types/stompjs": "^2.3.9",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/runtime-core": "^3.5.13",
    "@vue/runtime-dom": "^3.5.13",
    "babel-eslint": "^10.1.0",
    "esbuild-node-loader": "^0.8.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3",
    "typescript": "^5.4.3",
    "vite": "^5.4.11"
  }
}
